--text code

function text_setup()
	texts={}
	add_text(1,2,"first sign!")
	add_text(8,2,"oh, look!\na sign")
	add_text(52,14,"oh,a target!\ngo there to win!")	
end

function add_text(x,y,message)
	texts[x+y*128]=message
end

function get_text(x,y)
	return texts[x+y*128]
end

function draw_text()
	if (active_text) then
		textx=mapx*8+4
		texty=mapy*8+48
		
		rectfill(textx,texty,textx+119,texty+40,7)
		print(active_text,textx+4,texty+4,1)
		print("🅾️ to close",textx+4,texty+33,6)
	end
	
	if (btnp(🅾️)) active_text=nil
end
