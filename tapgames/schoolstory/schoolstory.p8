--map code

function map_setup()
	--timers
	timer=0
	anim_time=30 --30 = 1 second


	--map tile settings
	wall={18,23,24,25,26,32,34,35,36,37,50,51,59,61,62,63,47,56,33}
	door={52,54}
	book={20}
	anim1={34,38,40,27}
	anim2={35,39,41,28}
	lose={27,39,41}
	win={9}
	floor={49}
	path={48}
	locker={46}
end

function update_map()
	if (timer<0) then
		toggle_tiles()
		timer=anim_time
		end
		timer-=1
end


function draw_map()
	mapx=flr(p.x/16)*16
	mapy=flr(p.y/16)*16
	camera(mapx*8,mapy*8)
	
	map(0,0,0,0,128,64)
end

function is_tile(tile_type,x,y)
	tile=mget(x,y)
	for i=1,#tile_type do
		if (tile==tile_type[i]) return true		
	end
	return false
end

function can_move(x,y)
	return not is_tile(wall,x,y)
end

function swap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile+1)
end

function unswap_tile(x,y)
	tile=mget(x,y)
	mset(x,y,tile-1)
end



